<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MRS\ControleBundle\Entity\Product" table="tb_product" repository-class="MRS\ControleBundle\Repository\ProductRepository">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="imageName" type="string" column="imageName" length="255">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="updatedAt" type="datetime" column="updateAt">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
