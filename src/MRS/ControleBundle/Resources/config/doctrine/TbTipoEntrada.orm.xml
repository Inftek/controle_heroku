<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MRS\ControleBundle\Entity\TbTipoEntrada" table="tb_tipo_entrada">
    <id name="tenCodigo" type="integer" column="ten_codigo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tenDescricao" type="string" column="ten_descricao" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tenAtivo" type="string" column="ten_ativo" length="1" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
